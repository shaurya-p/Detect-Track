[project]
name = "gnd-dino-bytetrack"
version = "0.1.0"
description = "grounding dino + bytetrack implementation"
authors = [
    {name = "Shaurya Parashar",email = "shauryaparashar@gmail.com"}
]
readme = "README.md"
requires-python = ">=3.10,<3.12"
dependencies = [
    "torch @ https://download.pytorch.org/whl/cu121/torch-2.1.0%2Bcu121-cp310-cp310-win_amd64.whl",
    "torchvision @ https://download.pytorch.org/whl/cu121/torchvision-0.16.0%2Bcu121-cp310-cp310-win_amd64.whl",
    "torchaudio @ https://download.pytorch.org/whl/cu121/torchaudio-2.1.0%2Bcu121-cp310-cp310-win_amd64.whl",
    "opencv-python (>=4.11.0.86,<5.0.0.0)",
    "matplotlib (>=3.10.1,<4.0.0)",
    "scipy (>=1.15.2,<2.0.0)",
    "scikit-learn (>=1.6.1,<2.0.0)",
    "tqdm (>=4.67.1,<5.0.0)",
    "pycocotools (>=2.0.8,<3.0.0)",
    "filterpy (>=1.4.5,<2.0.0)",
    "numpy (>=1.24,<2.0)",
    "loguru (>=0.7.3,<0.8.0)",
    "thop (>=0.1.1.post2209072238,<0.2.0)",
    "transformers (>=4.51.3,<5.0.0)",
    "addict (>=2.4.0,<3.0.0)",
    "yapf (>=0.43.0,<0.44.0)",
    "timm (>=1.0.15,<2.0.0)",
    "supervision (>=0.25.1,<0.26.0)",
    "setuptools (>=80.0.0,<81.0.0)"
]


[build-system]
requires = ["poetry-core>=2.0.0,<3.0.0"]
build-backend = "poetry.core.masonry.api"
